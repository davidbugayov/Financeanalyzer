  <!-- Sberbank Instructions -->
  <string name="sberbank_instructions_title">–ß–µ—Ä–µ–∑ –º–æ–±–∏–ª—å–Ω–æ–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –°–±–µ—Ä–±–∞–Ω–∫:</string>
  <string name="sberbank_instructions">1. –í–æ–π–¥–∏—Ç–µ –≤ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –°–±–µ—Ä–±–∞–Ω–∫\n2. –ù–∞–∂–º–∏—Ç–µ –Ω–∞ –∫–∞—Ä—Ç—É, –ø–æ –∫–æ—Ç–æ—Ä–æ–π –Ω—É–∂–Ω–∞ –≤—ã–ø–∏—Å–∫–∞\n3. –ù–∞–∂–º–∏—Ç–µ –Ω–∞ –∫–Ω–æ–ø–∫—É \"–î–µ—Ç–∞–ª–∏\" (—Ç—Ä–∏ —Ç–æ—á–∫–∏ –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É)\n4. –í—ã–±–µ—Ä–∏—Ç–µ \"–í—ã–ø–∏—Å–∫–∞ –ø–æ —Å—á–µ—Ç—É\"\n5. –£–∫–∞–∂–∏—Ç–µ –ø–µ—Ä–∏–æ–¥, –∑–∞ –∫–æ—Ç–æ—Ä—ã–π –Ω—É–∂–Ω–∞ –≤—ã–ø–∏—Å–∫–∞\n6. –ù–∞–∂–º–∏—Ç–µ \"–°—Ñ–æ—Ä–º–∏—Ä–æ–≤–∞—Ç—å –≤—ã–ø–∏—Å–∫—É\"\n7. –ü–æ—Å–ª–µ —Ñ–æ—Ä–º–∏—Ä–æ–≤–∞–Ω–∏—è –Ω–∞–∂–º–∏—Ç–µ \"–°–∫–∞—á–∞—Ç—å PDF\"\n8. –°–æ—Ö—Ä–∞–Ω–∏—Ç–µ —Ñ–∞–π–ª –Ω–∞ —É—Å—Ç—Ä–æ–π—Å—Ç–≤–æ\n9. –ò–º–ø–æ—Ä—Ç–∏—Ä—É–π—Ç–µ —ç—Ç–æ—Ç PDF-—Ñ–∞–π–ª –≤ –Ω–∞—à–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ</string>
  <string name="sberbank_note">–ù–∞—à–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç –∏–º–ø–æ—Ä—Ç PDF-–≤—ã–ø–∏—Å–æ–∫ –°–±–µ—Ä–±–∞–Ω–∫–∞ –Ω–∞–ø—Ä—è–º—É—é. –ü—Ä–æ—Å—Ç–æ –≤—ã–±–µ—Ä–∏—Ç–µ —Å–∫–∞—á–∞–Ω–Ω—ã–π PDF-—Ñ–∞–π–ª –∏ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Ä–∞—Å–ø–æ–∑–Ω–∞–µ—Ç –≤—Å–µ —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–∏.</string>

  <!-- Tinkoff Instructions -->
  <string name="tinkoff_instructions_title">–ß–µ—Ä–µ–∑ –º–æ–±–∏–ª—å–Ω–æ–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –¢–∏–Ω—å–∫–æ—Ñ—Ñ:</string>
  <string name="tinkoff_instructions">1. –í–æ–π–¥–∏—Ç–µ –≤ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –¢–∏–Ω—å–∫–æ—Ñ—Ñ\n2. –í—ã–±–µ—Ä–∏—Ç–µ –∫–∞—Ä—Ç—É –∏–ª–∏ —Å—á–µ—Ç\n3. –ù–∞–∂–º–∏—Ç–µ –Ω–∞ —Ç—Ä–∏ —Ç–æ—á–∫–∏ –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É\n4. –í—ã–±–µ—Ä–∏—Ç–µ \"–í—ã–ø–∏—Å–∫–∞ –∏ —Å–ø—Ä–∞–≤–∫–∏\"\n5. –í—ã–±–µ—Ä–∏—Ç–µ \"–í—ã–ø–∏—Å–∫–∞ –ø–æ —Å—á–µ—Ç—É\"\n6. –í—ã–±–µ—Ä–∏—Ç–µ –ø–µ—Ä–∏–æ–¥ –≤—ã–ø–∏—Å–∫–∏ (–Ω–∞–ø—Ä–∏–º–µ—Ä, –∑–∞ –ø–æ—Å–ª–µ–¥–Ω–∏–π –º–µ—Å—è—Ü)\n7. –ù–∞–∂–º–∏—Ç–µ \"–ó–∞–∫–∞–∑–∞—Ç—å –≤—ã–ø–∏—Å–∫—É\"\n8. –î–æ–∂–¥–∏—Ç–µ—Å—å —Ñ–æ—Ä–º–∏—Ä–æ–≤–∞–Ω–∏—è –≤—ã–ø–∏—Å–∫–∏ (–æ–±—ã—á–Ω–æ –∑–∞–Ω–∏–º–∞–µ—Ç –Ω–µ—Å–∫–æ–ª—å–∫–æ –º–∏–Ω—É—Ç)\n9. –û—Ç–∫—Ä–æ–π—Ç–µ –ø–æ–ª—É—á–µ–Ω–Ω—É—é –≤—ã–ø–∏—Å–∫—É –∏ –Ω–∞–∂–º–∏—Ç–µ –Ω–∞ –∑–Ω–∞—á–æ–∫ \"–ü–æ–¥–µ–ª–∏—Ç—å—Å—è\"\n10. –°–æ—Ö—Ä–∞–Ω–∏—Ç–µ PDF-—Ñ–∞–π–ª –Ω–∞ —É—Å—Ç—Ä–æ–π—Å—Ç–≤–æ\n11. –ò–º–ø–æ—Ä—Ç–∏—Ä—É–π—Ç–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã–π —Ñ–∞–π–ª –≤ –Ω–∞—à–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ\n\n–ü—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Ä–∞—Å–ø–æ–∑–Ω–∞–µ—Ç –Ω–æ–≤—ã–π —Ñ–æ—Ä–º–∞—Ç –≤—ã–ø–∏—Å–æ–∫ –¢–∏–Ω—å–∫–æ—Ñ—Ñ –∏ –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ –∏–º–ø–æ—Ä—Ç–∏—Ä—É–µ—Ç –≤—Å–µ —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–∏.</string>

  <!-- Alfa-Bank Instructions -->
  <string name="alfabank_instructions_title_mobile">–ß–µ—Ä–µ–∑ –º–æ–±–∏–ª—å–Ω–æ–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –ê–ª—å—Ñ–∞-–ë–∞–Ω–∫:</string>
  <string name="alfabank_instructions_mobile">1. –í–æ–π–¥–∏—Ç–µ –≤ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –ê–ª—å—Ñ–∞-–ë–∞–Ω–∫\n2. –í—ã–±–µ—Ä–∏—Ç–µ –∫–∞—Ä—Ç—É –∏–ª–∏ —Å—á–µ—Ç\n3. –ù–∞–∂–º–∏—Ç–µ –Ω–∞ –∑–Ω–∞—á–æ–∫ \"–í—ã–ø–∏—Å–∫–∞\" (–∏–∫–æ–Ω–∫–∞ –¥–æ–∫—É–º–µ–Ω—Ç–∞)\n4. –£–∫–∞–∂–∏—Ç–µ –ø–µ—Ä–∏–æ–¥ –≤—ã–ø–∏—Å–∫–∏\n5. –ù–∞–∂–º–∏—Ç–µ \"–°—Ñ–æ—Ä–º–∏—Ä–æ–≤–∞—Ç—å\"\n6. –ü–æ—Å–ª–µ —Ñ–æ—Ä–º–∏—Ä–æ–≤–∞–Ω–∏—è –Ω–∞–∂–º–∏—Ç–µ \"–°–∫–∞—á–∞—Ç—å\"\n7. –í—ã–±–µ—Ä–∏—Ç–µ —Ñ–æ—Ä–º–∞—Ç XLSX (Excel)\n8. –°–æ—Ö—Ä–∞–Ω–∏—Ç–µ —Ñ–∞–π–ª –Ω–∞ —É—Å—Ç—Ä–æ–π—Å—Ç–≤–æ\n9. –ò–º–ø–æ—Ä—Ç–∏—Ä—É–π—Ç–µ —ç—Ç–æ—Ç XLSX-—Ñ–∞–π–ª –≤ –Ω–∞—à–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ</string>
  <string name="alfabank_instructions_title_web">–ß–µ—Ä–µ–∑ –ê–ª—å—Ñ–∞-–ë–∞–Ω–∫ –û–Ω–ª–∞–π–Ω (–≤–µ–±-–≤–µ—Ä—Å–∏—è):</string>
  <string name="alfabank_instructions_web">1. –í–æ–π–¥–∏—Ç–µ –≤ –ª–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç –ê–ª—å—Ñ–∞-–ë–∞–Ω–∫ –û–Ω–ª–∞–π–Ω\n2. –í—ã–±–µ—Ä–∏—Ç–µ —Å—á–µ—Ç –∏–ª–∏ –∫–∞—Ä—Ç—É\n3. –ù–∞–∂–º–∏—Ç–µ –Ω–∞ –∫–Ω–æ–ø–∫—É \"–í—ã–ø–∏—Å–∫–∞\"\n4. –£–∫–∞–∂–∏—Ç–µ –ø–µ—Ä–∏–æ–¥ –≤—ã–ø–∏—Å–∫–∏\n5. –ù–∞–∂–º–∏—Ç–µ \"–°—Ñ–æ—Ä–º–∏—Ä–æ–≤–∞—Ç—å –≤—ã–ø–∏—Å–∫—É\"\n6. –í—ã–±–µ—Ä–∏—Ç–µ —Ñ–æ—Ä–º–∞—Ç \"Excel\"\n7. –ù–∞–∂–º–∏—Ç–µ \"–°–∫–∞—á–∞—Ç—å\"\n8. –ò–º–ø–æ—Ä—Ç–∏—Ä—É–π—Ç–µ —Å–∫–∞—á–∞–Ω–Ω—ã–π XLSX-—Ñ–∞–π–ª –≤ –Ω–∞—à–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ</string>
  <string name="alfabank_note_important">–í–∞–∂–Ω–æ:</string>
  <string name="alfabank_note">–ù–∞—à–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç –ø—Ä—è–º–æ–π –∏–º–ø–æ—Ä—Ç Excel-—Ñ–∞–π–ª–æ–≤ (.xlsx) –∏–∑ –ê–ª—å—Ñ–∞-–ë–∞–Ω–∫–∞. –ü—Ä–æ—Å—Ç–æ –≤—ã–±–µ—Ä–∏—Ç–µ —Å–∫–∞—á–∞–Ω–Ω—ã–π —Ñ–∞–π–ª, –∏ –≤—Å–µ —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–∏ –±—É–¥—É—Ç –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Ä–∞—Å–ø–æ–∑–Ω–∞–Ω—ã.</string>

